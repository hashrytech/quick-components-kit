{
	"name": "@hashrytech/quick-components-kit",
	"description": "A Svelte component library and toolset",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/hashrytech/quick-components-kit.git"
	},
	"version": "0.19.2",
	"license": "MIT",
	"author": "Hashry Tech",
	"scripts": {
		"dev": "vite dev --host --port 2000",
		"build": "vite build && npm run prepack",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"prepack": "svelte-kit sync && svelte-package && publint",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run && npm run test:e2e",
		"test:e2e": "playwright test",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build",
		"change": "npx changeset add",
		"version": "npx changeset version",
		"package": "svelte-package",
		"clean-package-json": "node ./scripts/clean-package-json.js"
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*",
		"LICENSE",
		"README.md",
		"CHANGELOG.md"
	],
	"sideEffects": [
		"**/*.css"
	],
	"publishConfig": {
		"access": "public"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		},
		"./tailwind": "./dist/quick-components-kit-plugin.js"
	},
	"peerDependencies": {
		"svelte": "^5.0.0"
	},
	"devDependencies": {
		"@changesets/cli": "^2.29.4",
		"@chromatic-com/storybook": "^4.0.0",
		"@eslint/compat": "^1.2.9",
		"@eslint/js": "^9.27.0",
		"@playwright/test": "^1.52.0",
		"@storybook/addon-a11y": "^9.0.0",
		"@storybook/addon-docs": "^9.0.0",
		"@storybook/addon-svelte-csf": "^5.0.3",
		"@storybook/addon-vitest": "^9.0.0",
		"@storybook/sveltekit": "^9.0.0",
		"@sveltejs/adapter-auto": "^6.0.1",
		"@sveltejs/kit": "^2.21.1",
		"@sveltejs/package": "^2.3.11",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/vite": "^4.1.8",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/svelte": "^5.2.8",
		"@types/node": "^22",
		"@vitest/browser": "^3.1.4",
		"@vitest/coverage-v8": "^3.1.4",
		"eslint": "^9.27.0",
		"eslint-config-prettier": "^10.1.5",
		"eslint-plugin-storybook": "^9.0.0",
		"eslint-plugin-svelte": "^3.9.0",
		"globals": "^16.2.0",
		"jsdom": "^26.1.0",
		"playwright": "^1.52.0",
		"prettier": "^3.5.3",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"publint": "^0.3.12",
		"storybook": "^9.0.0",
		"svelte": "^5.33.5",
		"svelte-check": "^4.2.1",
		"svelte-remixicon": "^2.7.0",
		"tailwindcss": "^4.1.8",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.33.0",
		"vite": "^6.3.5",
		"vitest": "^3.1.4"
	},
	"keywords": [
		"svelte"
	],
	"dependencies": {
		"tailwind-merge": "^3.3.0"
	}
}
